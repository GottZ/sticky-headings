{
	"name": "sticky-headings",
	"version": "0.0.1",
	"description": "Magical plugin to give a context-aware peek at where in the document structure you currently are. (similar to the GitHub sourcecode browser)",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": ["sticky", "head", "heading", "obsidian", "navigation", "context"],
	"author": "Jan-Stefan Janetzky <github@gottz.de>",
	"license": "MIT",
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/GottZ"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/GottZ/sticky-headings"
	},
	"private": true,
	"bugs": {
		"url": "https://github.com/GottZ/sticky-headings/issues"
	},
	"homepage": "https://github.com/GottZ/sticky-headings/blob/root/README.md",
	"devDependencies": {
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "7.6.0",
		"@typescript-eslint/parser": "7.6.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.20.2",
		"obsidian": "latest",
		"tslib": "2.6.2",
		"typescript": "5.4.5"
	}
}
